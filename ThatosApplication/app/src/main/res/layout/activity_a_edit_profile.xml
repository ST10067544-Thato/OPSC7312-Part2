<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumnhomeOne"
        style="@style/groupStyledeep_orange_400"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnarrowleft"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="61dp"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRowarrowleft"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="37dp"
                    android:layout_marginEnd="37dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArrowleftOne"
                        android:layout_width="19dp"
                        android:layout_height="16dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="6dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_left_white_a700_16x19"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrow_left_white_a700_16x19" />

                    <TextView
                        android:id="@+id/txtEditmyprofile"
                        style="@style/txtPoppinssemibold20_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="87dp"
                        android:ellipsize="end"
                        android:lineSpacingExtra="10dp"
                        android:singleLine="true"
                        android:text="@string/lbl_edit_my_profile"
                        tools:text="@string/lbl_edit_my_profile" />

                    <ImageButton
                        android:id="@+id/btnVectorOne"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="65dp"
                        android:background="@drawable/rectangle_bg_red_50_radius_15"
                        android:paddingHorizontal="5dp"
                        android:paddingVertical="5dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_blue_gray_900_01"
                        tools:ignore="ContentDescription" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <FrameLayout
                android:id="@+id/frameStackthumbsup"
                android:layout_width="117dp"
                android:layout_height="117dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25dp">

                <ImageView
                    android:id="@+id/imageCircleimage"
                    app:circular="@{true}"
                    app:imageRes="@{@drawable/img_ellipse_192}"
                    android:layout_width="117dp"
                    android:layout_height="117dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_ellipse_192" />

                <ImageButton
                    android:id="@+id/btnThumbsupOne"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="bottom|end"
                    android:layout_marginEnd="13dp"
                    android:background="@drawable/rectangle_bg_deep_orange_400_radius_12"
                    android:paddingHorizontal="5dp"
                    android:paddingVertical="5dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_thumbs_up_blue_gray_900_01"
                    tools:ignore="ContentDescription" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearColumnjohnsmith"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="36dp"
            android:paddingTop="75dp"
            android:paddingEnd="36dp"
            android:paddingBottom="75dp">

            <TextView
                android:id="@+id/txtJohnsmith"
                style="@style/txtPoppinsbold20_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_john_smith"
                tools:text="@string/lbl_john_smith" />

            <TextView
                android:id="@+id/txtAccount"
                style="@style/txtPoppinssemibold20_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="2dp"
                android:layout_marginTop="49dp"
                android:ellipsize="end"
                android:lineSpacingExtra="10dp"
                android:singleLine="true"
                android:text="@string/msg_account_settings"
                tools:text="@string/msg_account_settings" />

            <LinearLayout
                android:id="@+id/linearColumnusername"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginTop="20dp"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtUsername"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_username"
                    tools:text="@string/lbl_username" />

                <EditText
                    android:id="@+id/etUserName"
                    style="@style/etSolidRounded_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/rectangle_bg_red_50_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_john_smith"
                    android:inputType="text"
                    android:paddingStart="20dp"
                    android:paddingTop="6dp"
                    android:paddingEnd="20dp"
                    android:paddingBottom="6dp"
                    android:singleLine="true"
                    android:textColorHint="@color/blue_gray_900_01"
                    tools:ignore="Autofill" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnphone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginTop="17dp"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtPhone"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_phone"
                    tools:text="@string/lbl_phone" />

                <EditText
                    android:id="@+id/etPhone"
                    style="@style/etSolidRounded_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/rectangle_bg_red_50_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_44_555_5555_55"
                    android:inputType="text"
                    android:paddingStart="20dp"
                    android:paddingTop="6dp"
                    android:paddingEnd="20dp"
                    android:paddingBottom="6dp"
                    android:singleLine="true"
                    android:textColorHint="@color/teal_900"
                    tools:ignore="Autofill" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnemailaddr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginTop="15dp"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtEmailaddress"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_email_address"
                    tools:text="@string/lbl_email_address" />

                <EditText
                    android:id="@+id/etEmail"
                    style="@style/etSolidRounded_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/rectangle_bg_red_50_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/msg_example_example_com"
                    android:inputType="text"
                    android:paddingStart="20dp"
                    android:paddingTop="6dp"
                    android:paddingEnd="20dp"
                    android:paddingBottom="6dp"
                    android:singleLine="true"
                    android:textColorHint="@color/blue_gray_900_01"
                    tools:ignore="Autofill" />
            </LinearLayout>

            <View
                android:id="@+id/viewView"
                android:layout_width="169dp"
                android:layout_height="144dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnUpdateProfile"
                style="@style/btnSolidRounded_4"
                android:layout_width="169dp"
                android:layout_height="36dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="66dp"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="30dp"
                android:paddingTop="5dp"
                android:paddingEnd="30dp"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text="@string/lbl_update_profile"
                android:textAllCaps="false"
                tools:text="@string/lbl_update_profile" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylered_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <LinearLayout
                android:id="@+id/linearRowhomeOne"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="41dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_gray_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_gray_900" />

                <ImageView
                    android:id="@+id/imageSettingsOne"
                    android:layout_width="31dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_settings" />

                <ImageView
                    android:id="@+id/imageQuestionOne"
                    android:layout_width="33dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_question"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_question" />

                <ImageView
                    android:id="@+id/imageThumbsupThree"
                    android:layout_width="27dp"
                    android:layout_height="23dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbs_up"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_thumbs_up" />

                <ImageView
                    android:id="@+id/imagecloseOne"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close_deep_orange_400"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_close_deep_orange_400" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
