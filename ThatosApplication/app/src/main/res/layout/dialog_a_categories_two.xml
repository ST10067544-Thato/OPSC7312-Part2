<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnnewcatego"
            style="@style/groupStylewhite_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="45dp"
            android:layout_marginEnd="45dp"
            android:orientation="vertical"
            android:paddingStart="32dp"
            android:paddingTop="69dp"
            android:paddingEnd="32dp"
            android:paddingBottom="69dp">

            <TextView
                android:id="@+id/txtNewcategory"
                style="@style/txtPoppinsbold20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="2dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_new_category"
                tools:text="@string/lbl_new_category" />

            <EditText
                android:id="@+id/etWritevalueOne"
                style="@style/etSolidRounded_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="7dp"
                android:background="@drawable/rectangle_bg_red_50_radius_18"
                android:ellipsize="end"
                android:hint="@string/lbl_write"
                android:inputType="text"
                android:paddingStart="17dp"
                android:paddingTop="12dp"
                android:paddingEnd="17dp"
                android:paddingBottom="12dp"
                android:singleLine="true"
                android:textColorHint="@color/deep_orange_400"
                tools:ignore="Autofill" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnSave"
                style="@style/btnSolidRounded_5"
                android:layout_width="218dp"
                android:layout_height="45dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="25dp"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="32dp"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="30dp"
                android:paddingTop="10dp"
                android:paddingEnd="30dp"
                android:paddingBottom="10dp"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                tools:text="@string/lbl_save" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnCancel"
                style="@style/btnSolidRounded_6"
                android:layout_width="218dp"
                android:layout_height="45dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="25dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="32dp"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="30dp"
                android:paddingTop="10dp"
                android:paddingEnd="30dp"
                android:paddingBottom="10dp"
                android:singleLine="true"
                android:text="@string/lbl_cancel"
                android:textAllCaps="false"
                tools:text="@string/lbl_cancel" />
        </LinearLayout>
    </LinearLayout>
</layout>