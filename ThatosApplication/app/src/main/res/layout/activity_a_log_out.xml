<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumnhomeOne"
        style="@style/groupStyledeep_orange_400"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnarrowleft"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRowarrowleft"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="37dp"
                    android:layout_marginEnd="37dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArrowleftOne"
                        android:layout_width="19dp"
                        android:layout_height="16dp"
                        android:layout_marginTop="9dp"
                        android:layout_marginBottom="6dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_left_white_a700_16x19"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrow_left_white_a700_16x19" />

                    <TextView
                        android:id="@+id/txtProfile"
                        style="@style/txtPoppinssemibold20_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="126dp"
                        android:layout_marginBottom="1dp"
                        android:ellipsize="end"
                        android:lineSpacingExtra="10dp"
                        android:singleLine="true"
                        android:text="@string/lbl_profile"
                        tools:text="@string/lbl_profile" />

                    <ImageButton
                        android:id="@+id/btnVectorOne"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="116dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/rectangle_bg_deep_orange_50_radius_15"
                        android:paddingHorizontal="5dp"
                        android:paddingVertical="5dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_blue_gray_900_01"
                        tools:ignore="ContentDescription" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <ImageView
                android:id="@+id/imageCircleimage"
                app:circular="@{true}"
                app:imageRes="@{@drawable/img_ellipse_192}"
                android:layout_width="117dp"
                android:layout_height="117dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="26dp"
                android:scaleType="centerCrop"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_ellipse_192" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearColumnjohnsmith"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:orientation="vertical"
            android:paddingStart="38dp"
            android:paddingTop="75dp"
            android:paddingEnd="38dp"
            android:paddingBottom="75dp">

            <TextView
                android:id="@+id/txtJohnsmith"
                style="@style/txtPoppinsbold20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_john_smith"
                tools:text="@string/lbl_john_smith" />

            <LinearLayout
                android:id="@+id/linearRowlockOne"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="68dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameIconprofile"
                    style="@style/groupStyleblue_3"
                    android:layout_width="57dp"
                    android:layout_height="53dp"
                    android:layout_gravity="center_vertical"
                    android:paddingStart="17dp"
                    android:paddingTop="12dp"
                    android:paddingEnd="17dp"
                    android:paddingBottom="12dp">

                    <ImageView
                        android:id="@+id/imageLockOne"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_gravity="top|center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock_white_a700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_lock_white_a700" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtEditprofile"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="14dp"
                    android:layout_marginBottom="15dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/lbl_edit_profile"
                    tools:text="@string/lbl_edit_profile" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowcheckmark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="34dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameIconsecurity"
                    style="@style/groupStyleblue_3"
                    android:layout_width="57dp"
                    android:layout_height="53dp"
                    android:layout_gravity="center_vertical"
                    android:paddingStart="15dp"
                    android:paddingTop="15dp"
                    android:paddingEnd="15dp"
                    android:paddingBottom="12dp">

                    <ImageView
                        android:id="@+id/imageCheckmarkOne"
                        android:layout_width="26dp"
                        android:layout_height="28dp"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_checkmark"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_checkmark" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtSecurity"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="13dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_security"
                    tools:text="@string/lbl_security" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowtelevision"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="34dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameIconsetting"
                    style="@style/groupStyleblue_3"
                    android:layout_width="57dp"
                    android:layout_height="53dp"
                    android:layout_gravity="center_vertical"
                    android:paddingStart="14dp"
                    android:paddingTop="12dp"
                    android:paddingEnd="14dp"
                    android:paddingBottom="12dp">

                    <ImageView
                        android:id="@+id/imageTelevisionOne"
                        android:layout_width="27dp"
                        android:layout_height="28dp"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_television_white_a700_28x27"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_television_white_a700_28x27" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtSetting"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="13dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_setting"
                    tools:text="@string/lbl_setting" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowarrowleft1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="34dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameIconlogout"
                    style="@style/groupStylered_500cornerRadius"
                    android:layout_width="57dp"
                    android:layout_height="53dp"
                    android:layout_gravity="center_vertical"
                    android:paddingStart="19dp"
                    android:paddingTop="13dp"
                    android:paddingEnd="19dp"
                    android:paddingBottom="13dp">

                    <ImageView
                        android:id="@+id/imageArrowleftThree"
                        android:layout_width="18dp"
                        android:layout_height="26dp"
                        android:layout_gravity="end"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_left_white_a700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrow_left_white_a700" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtLogout"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="13dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_logout"
                    tools:text="@string/lbl_logout" />
            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylered_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <LinearLayout
                android:id="@+id/linearRowhomeOne"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="41dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_gray_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_gray_900" />

                <ImageView
                    android:id="@+id/imageSettingsOne"
                    android:layout_width="31dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_settings" />

                <ImageView
                    android:id="@+id/imageQuestionOne"
                    android:layout_width="33dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_question"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_question" />

                <ImageView
                    android:id="@+id/imageThumbsupOne"
                    android:layout_width="27dp"
                    android:layout_height="23dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbs_up"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_thumbs_up" />

                <ImageView
                    android:id="@+id/imagecloseOne"
                    android:layout_width="22dp"
                    android:layout_height="27dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close_deep_orange_400"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_close_deep_orange_400" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>