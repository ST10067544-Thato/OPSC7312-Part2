<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumnarrowLeftOne"
        style="@style/groupStyledeep_orange_400"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearRowarrowleft"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="37dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="37dp"
                android:layout_marginBottom="12dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageArrowleftOne"
                    android:layout_width="19dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="6dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_left_white_a700_16x19"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrow_left_white_a700_16x19" />

                <TextView
                    android:id="@+id/txtCategories"
                    style="@style/txtPoppinssemibold20_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="96dp"
                    android:ellipsize="end"
                    android:lineSpacingExtra="10dp"
                    android:singleLine="true"
                    android:text="@string/lbl_categories"
                    tools:text="@string/lbl_categories" />

                <ImageButton
                    android:id="@+id/btnVectorOne"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="78dp"
                    android:background="@drawable/rectangle_bg_deep_orange_50_radius_15"
                    android:paddingHorizontal="5dp"
                    android:paddingVertical="5dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector_blue_gray_900_01"
                    tools:ignore="ContentDescription" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearColumnvector"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:orientation="vertical"
            android:paddingStart="36dp"
            android:paddingTop="71dp"
            android:paddingEnd="36dp"
            android:paddingBottom="71dp">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerGridfoodtext"
                app:itemSpace="@{@dimen/_21pxh}"
                app:spanCount="3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:gravity="center_horizontal"
                app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                tools:itemCount="9"
                tools:listitem="@layout/row_gridfoodtext" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylered_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <LinearLayout
                android:id="@+id/linearRowhomeOne"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="41dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_gray_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_gray_900" />

                <ImageView
                    android:id="@+id/imageSettingsOne"
                    android:layout_width="31dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_settings" />

                <ImageView
                    android:id="@+id/imageQuestionOne"
                    android:layout_width="33dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_question"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_question" />

                <ImageView
                    android:id="@+id/imagecloseOne"
                    android:layout_width="27dp"
                    android:layout_height="23dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_close" />

                <ImageView
                    android:id="@+id/imageLockOne"
                    android:layout_width="22dp"
                    android:layout_height="27dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_lock" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>