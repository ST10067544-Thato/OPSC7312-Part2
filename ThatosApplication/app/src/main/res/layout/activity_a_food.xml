<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumnarrowLeftOne"
        style="@style/groupStyledeep_orange_400"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearRowarrowleft"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="37dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="37dp"
                android:layout_marginBottom="12dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageArrowleftOne"
                    android:layout_width="19dp"
                    android:layout_height="16dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="6dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_left_white_a700_16x19"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrow_left_white_a700_16x19" />

                <TextView
                    android:id="@+id/txtFood"
                    style="@style/txtPoppinssemibold20_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="132dp"
                    android:layout_marginBottom="1dp"
                    android:ellipsize="end"
                    android:lineSpacingExtra="10dp"
                    android:singleLine="true"
                    android:text="@string/lbl_food"
                    tools:text="@string/lbl_food" />

                <ImageButton
                    android:id="@+id/btnVectorOne"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="98dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/rectangle_bg_red_50_radius_15"
                    android:paddingHorizontal="5dp"
                    android:paddingVertical="5dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector_blue_gray_900_01"
                    tools:ignore="ContentDescription" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearColumnapril"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="start"
            android:orientation="vertical"
            android:paddingStart="33dp"
            android:paddingTop="49dp"
            android:paddingEnd="33dp"
            android:paddingBottom="49dp">

            <LinearLayout
                android:id="@+id/linearRowapril"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="13dp"
                android:layout_marginEnd="2dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtApril"
                    style="@style/txtPoppinsmedium15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="5dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_april"
                    tools:text="@string/lbl_april" />

                <ImageButton
                    android:id="@+id/btnCalendarOne"
                    android:layout_width="32dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="258dp"
                    android:background="@drawable/rectangle_bg_deep_orange_400_radius_12"
                    android:paddingHorizontal="6dp"
                    android:paddingVertical="6dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_calendar_red_50"
                    tools:ignore="ContentDescription" />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerorderitem"
                app:itemSpace="@{@dimen/_24pxh}"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:itemCount="4"
                tools:listitem="@layout/row_orderitem" />

            <TextView
                android:id="@+id/txtMarch"
                style="@style/txtPoppinsmedium15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="7dp"
                android:layout_marginTop="40dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_march"
                tools:text="@string/lbl_march" />

            <LinearLayout
                android:id="@+id/linearRowflashlight"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameIconfood"
                    style="@style/groupStyleblue_300cornerRadius"
                    android:layout_width="57dp"
                    android:layout_height="53dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="2dp"
                    android:paddingStart="20dp"
                    android:paddingTop="12dp"
                    android:paddingEnd="20dp"
                    android:paddingBottom="12dp">

                    <ImageView
                        android:id="@+id/imageFlashlightOne"
                        android:layout_width="15dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_flashlight"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_flashlight" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearColumndinner"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="11dp"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtDinner"
                        style="@style/txtPoppinsmedium15_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_dinner"
                        tools:text="@string/lbl_dinner" />

                    <TextView
                        android:id="@+id/txt2050marchthirty"
                        style="@style/txtPoppinssemibold12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_20_50_march_31"
                        tools:text="@string/msg_20_50_march_31" />
                </LinearLayout>

                <TextView
                    android:id="@+id/txtR27twenty"
                    style="@style/txtPoppinsmedium15_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_r27_20"
                    tools:text="@string/lbl_r27_20" />
            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylered_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <LinearLayout
                android:id="@+id/linearRowhomeOne"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="41dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_gray_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_gray_900" />

                <ImageView
                    android:id="@+id/imageSettingsOne"
                    android:layout_width="31dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_settings" />

                <ImageView
                    android:id="@+id/imageQuestionOne"
                    android:layout_width="33dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_question"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_question" />

                <ImageView
                    android:id="@+id/imagecloseOne"
                    android:layout_width="27dp"
                    android:layout_height="23dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_close" />

                <ImageView
                    android:id="@+id/imageLockOne"
                    android:layout_width="22dp"
                    android:layout_height="27dp"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_lock" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>